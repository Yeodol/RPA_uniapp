{
	"leftWindow": {
		"path": "windows/left-window.vue",
		"style": {
			"width": "350px"
		}
	},
	"topWindow": {
		"path": "windows/top-window.vue",
		"style": {
			"height": "60px"
		}
	},
	"pages": [
		{
			"path": "pages/tabBar/component/component",
			"style": {
				"navigationBarTitleText": "报告管理",
				"app-plus": {
					"bounce": "vertical",
					"titleNView": {
						"buttons": [{
							"text": "\ue534",
							"fontSrc": "/static/uni.ttf",
							"fontSize": "22px",
							"color": "#FFFFFF"
						}]
					}
				}
			}
		},
		{
			"path": "pages/tabBar/API/API",
			"style": {
				"navigationBarTitleText": "主页",
				"app-plus": {
					"titleNView": {
						"buttons": [{
							"text": "\ue534",
							"fontSrc": "/static/uni.ttf",
							"fontSize": "22px",
							"color": "#FFFFFF"
						}]
					}
				},
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/tabBar/template/template",
			"style": {
				"navigationBarTitleText": "设置",
				"app-plus": {
					"titleNView": {
						"buttons": [{
							"text": "\ue534",
							"fontSrc": "/static/uni.ttf",
							"fontSize": "22px",
							"color": "#FFFFFF"
						}]
					}
				}
			}
		},
		{
			"path": "pages/analysis/analysis",
			"style": {
				"navigationBarTitleText": "PCR数据分析结果"
			}
		},
		{
			"path": "pages/pdf-viewer",
			"style": {
				"navigationBarTitleText": "PDF报告查看器",
				"app-plus": {
					"bounce": "none"
				}
			}
		},
		{
			"path": "pages/template/company/company",
			"style": {
				"navigationBarTitleText": "公司简介"
			}
		},
		{
			"path": "pages/template/database/database",
			"style": {
				"navigationBarTitleText": "数据库设置"
			}
		},
		{
			"path": "pages/template/account/account",
			"style": {
				"navigationBarTitleText": "账号设置"
			}
		},
		{
			"path": "pages/template/version/version",
			"style": {
				"navigationBarTitleText": "检查版本"
